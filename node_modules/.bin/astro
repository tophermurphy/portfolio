#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/astro@5.0.9_rollup@4.28.1_sass@1.83.0_typescript@5.7.2/node_modules/astro/node_modules:/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/astro@5.0.9_rollup@4.28.1_sass@1.83.0_typescript@5.7.2/node_modules:/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/astro@5.0.9_rollup@4.28.1_sass@1.83.0_typescript@5.7.2/node_modules/astro/node_modules:/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/astro@5.0.9_rollup@4.28.1_sass@1.83.0_typescript@5.7.2/node_modules:/Volumes/BLUE/ExSites/upwork-portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
