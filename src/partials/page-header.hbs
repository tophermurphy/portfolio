<header class="page-header {{data.id}}-header">
    <div class="header-images">
                    {{#each data.headImages}}
                <picture class="img-wrapper image-{{../id}}-{{../dataKey}}-{{@key}}">
                    <img src="{{image filename}}" alt="{{alt}}" title="{{imagetitle filename}}"/>
                </picture>
            {{/each}}
    </div>
    <div class="page-title">
        <h1>{{data.title}}</h1>
        <h2>{{data.subtitle}}</h2>
    </div>
    <blockquote class="page-description">
        {{data.description}}
    </blockquote>
    <div class="page-info">
        <div class="page-roles">
           <strong>Roles:</strong> {{#each data.roles}}{{@this}}, {{/ each}}
        </div>
        <div class="page-tools">
            <strong>Tools:</strong> {{#each data.tools}}{{@this}}, {{/ each}}
        </div>
        <div class="page-platforms">
            <strong>Platform:</strong> {{#each data.platforms}}{{@this}}, {{/ each}}
        </div>
    </div>

</header>
