{{load this "lgb-cw"}}
{{load this "h2o-cw"}}

{{#> base baseData=h2oplussite }} 
    <main class="content content-page content-page-{{h2oplussite.id}}">
        {{> page-header data=h2oplussite dataKey="headImages" id=h2oplussite.id }}
   
    <article class="page-article {{h2oplussite.id}}-article">

        <section class="page-section section-custom section-text-mixed section1">
            <div class="section-images section-custom-images section-images-section1-0">
                <picture class="image-wrapper image-{{h2oplussite.id}} image-section1-0">
                    {{imagelink h2oplussite.content.section1.images.[0]}}
                </picture>
                <picture class="image-wrapper image-{{h2oplussite.id}} image-section1-1">
                    {{imagelink h2oplussite.content.section1.images.[1]}}
                </picture>
                <picture class="image-wrapper image-{{h2oplussite.id}} image-section1-2">
                    {{imagelink h2oplussite.content.section1.images.[2]}}
                </picture>
            </div>

            <div class="section-copy section-custom-copy section1-copy-0">
                <h3 class="section-copy-header">{{h2oplussite.content.section1.header}}</h3>
                <p class="section-copy-body">{{h2oplussite.content.section1.body.[0]}}</p>
                <p class="section-copy-body md-show">{{h2oplussite.content.section1.body.[1]}}</p>
            </div>

            <div class="section-images section-custom-images section-images-section1-1">
                <picture class="image-wrapper image-{{h2oplussite.id}} image-section1-3">
                    {{imagelink h2oplussite.content.section1.images.[3]}}
